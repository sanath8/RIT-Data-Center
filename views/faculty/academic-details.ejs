<% include ../partials/header %>
<% include ../partials/faculty-sidebar %>
<% include ../partials/faculty-navbar %>
<% include ../partials/loadTablePartial %>

<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
              <h2>Academic Details</h2>
                <div class="card">
                    <div class="card-header" data-background-color="purple">
                        <h4 class="title">Courses Handled</h4>
                        <p class="category">information about courses handled.</p>
                    </div>
                    <div class="card-content table-responsive">
                        <table class="table">
                            <thead class="text-primary">
                                <th>Year handled</th>
                                <th>Course name</th>
                                <th>UG / PG</th>
                                <th>Labs Handled</th>
                            </thead>
                            <tbody>
                                <% loadInTable(data.courses_handled,1)%>
                            </tbody>
                        </table>
                    </div>
                    <a href = "/faculty/generateexcel/<%- type %>/1">
                    <button class="btn btn-primary pull-left" style="margin:13px">Generate Excel file</button>
                    </a>
                    <button type="submit" class="btn btn-primary pull-right" style="margin:13px">Edit</button>
                </div>
                <div class="card">
                    <div class="card-header" data-background-color="purple">
                        <h4 class="title">Projects Handled</h4>
                        <p class="category">information about projects guided.</p>
                    </div>
                    <div class="card-content table-responsive">
                        <table class="table">
                            <thead class="text-primary">
                                <th>Batch</th>
                                <th>UG / PG</th>
                                <th>Project Title</th>
                            </thead>
                            <tbody>
                                <% loadInTable(data.projects_handled,2)%>
                            </tbody>
                        </table>
                    </div>
                    <a href = "/faculty/generateexcel/<%- type %>/2">
                    <button class="btn btn-primary pull-left" style="margin:13px">Generate Excel file</button>
                    </a>
                    <button type="submit" class="btn btn-primary pull-right" style="margin:13px">Edit</button>
                </div>
                <div class="card">
                    <div class="card-header" data-background-color="purple">
                        <h4 class="title">Faculty Research</h4>
                        <p class="category">information about faculty research etc.</p>
                    </div>
                    <div class="card-content table-responsive">
                        <table class="table">
                            <thead class="text-primary">
                                <th>Guide Name</th>
                                <th>Research Candidate Name</th>
                                <th>USN</th>
                                <th>Centre Name</th>
                                <th>University</th>
                                <th>Year of registration</th>
                                <th>Title</th>
                                <th>Status</th>
                            </thead>
                            <tbody>
                                <% loadInTable(data.faculty_research,3)%>
                            </tbody>
                        </table>
                    </div>
                    <a href = "/faculty/generateexcel/<%- type %>/3">
                    <button class="btn btn-primary pull-left" style="margin:13px">Generate Excel file</button>
                    </a>
                    <button type="submit" class="btn btn-primary pull-right" style="margin:13px">Edit</button>
                </div>
                <div class="card">
                    <div class="card-header" data-background-color="purple">
                        <h4 class="title">PhD Scholar</h4>
                        <p class="category">information about PhD scholars etc.</p>
                    </div>
                    <div class="card-content table-responsive">
                        <table class="table">
                            <thead class="text-primary">
                              <th>Research Scholar Name</th>
                              <th>Guide Name</th>
                              <th>Centre Name</th>
                              <th>University</th>
                              <th>Year of registration</th>
                              <th>USN</th>
                              <th>Title</th>
                              <th>Status</th>
                            </thead>
                            <tbody>
                                <% loadInTable(data.phd_scholar,4)%>
                            </tbody>
                        </table>
                    </div>
                    <a href = "/faculty/generateexcel/<%- type %>/4">
                    <button class="btn btn-primary pull-left" style="margin:13px">Generate Excel file</button>
                    </a>
                    <button type="submit" class="btn btn-primary pull-right" style="margin:13px">Edit</button>
                </div>
            </div>
        </div>
    </div>
</div>


