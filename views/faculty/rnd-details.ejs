<% include ../partials/header %>
<% include ../partials/faculty-sidebar %>
<% include ../partials/faculty-navbar %>
<% include ../partials/loadTablePartial %>

<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
              <h2>R&D Details</h2>
                <div class="card">
                    <div class="card-header" data-background-color="purple">
                        <h4 class="title">Funded Projects</h4>
                        <p class="category">information about projects handled.</p>
                    </div>
                    <div class="card-content table-responsive">
                        <table class="table">
                            <thead class="text-primary">
                                <th>Sl. No</th>
                                <th>Principal Investigator</th>
                                <th>Project Title</th>
                                <th>Funding Agency</th>
                                <th>Sanctioned Order NUmber</th>
                                <th>Project Duration</th>
                                <th>Sanctioned Date</th>
                                <th>Sanctioned Amount (in Rs.)</th>
                            </thead>
                            <tbody>
                                <% loadInTable(resultSet1, )%>
                            </tbody>
                        </table>
                    </div>
                    <a href = "/faculty/generateexcel/<%- type %>/1">
                    <button class="btn btn-primary pull-left" style="margin:13px">Generate Excel file</button>
                    </a>
                    <button type="submit" class="btn btn-primary pull-right" style="margin:13px">Edit</button>
                </div>
                <div class="card">
                    <div class="card-header" data-background-color="purple">
                        <h4 class="title">Patents</h4>
                        <p class="category">Patents applied for and granted.</p>
                    </div>
                    <div class="card-content table-responsive">
                        <table class="table">
                            <thead class="text-primary">
                                <th>Sl. No</th>
                                <th>Title</th>
                                <th>Application Number</th>
                                <th>Date of Filing Application</th>
                                <th>Publication Date</th>
                            </thead>
                            <tbody>
                                <% loadInTable(resultSet2)%>
                            </tbody>
                        </table>
                    </div>
                    <a href = "/faculty/generateexcel/<%- type %>/2">
                    <button class="btn btn-primary pull-left" style="margin:13px">Generate Excel file</button>
                    </a>
                    <button type="submit" class="btn btn-primary pull-right" style="margin:13px">Edit</button>
                </div><div class="card">
                    <div class="card-header" data-background-color="purple">
                        <h4 class="title">Consultancy Details</h4>
                        <p class="category">Consultancy for projects with organizations.</p>
                    </div>
                    <div class="card-content table-responsive">
                        <table class="table">
                            <thead class="text-primary">
                              <th>Sl. No</th>
                              <th>Financial Year</th>
                              <th>Client Orgnization</th>
                              <th>Title of Consultancy of Project</th>
                              <th>Amount received ( in Rs )</th>
                            </thead>
                            <tbody>
                                <% loadInTable(resultSet3)%>
                            </tbody>
                        </table>
                    </div>
                    <a href = "/faculty/generateexcel/<%- type %>/3">
                    <button class="btn btn-primary pull-left" style="margin:13px">Generate Excel file</button>
                    </a>
                    <button type="submit" class="btn btn-primary pull-right" style="margin:13px">Edit</button>
                </div>
                <div class="card">
                    <div class="card-header" data-background-color="purple">
                        <h4 class="title">Industrial Collaborations MOU</h4>
                        <p class="category">information about MOUs signed with industries</p>
                    </div>
                    <div class="card-content table-responsive">
                        <table class="table">
                            <thead class="text-primary">
                              <th>Sl. No</th>
                              <th>Title of MOU</th>
                              <th>MOU signed with Industry</th>
                              <th>Signing Date</th>
                            </thead>
                            <tbody>
                                <% loadInTable(resultSet4)%>
                            </tbody>
                        </table>
                    </div>
                    <a href = "/faculty/generateexcel/<%- type %>/4">
                    <button class="btn btn-primary pull-left" style="margin:13px">Generate Excel file</button>
                    </a>
                    <button type="submit" class="btn btn-primary pull-right" style="margin:13px">Edit</button>
                </div>
            </div>
        </div>
    </div>
</div>

<% include ../partials/footer %>
