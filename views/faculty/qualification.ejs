
<% include ../partials/header %>
<% include ../partials/faculty-sidebar %>
<% include ../partials/faculty-navbar %>
<% include ../partials/loadTablePartial %>

<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header" data-background-color="purple">
                        <h4 class="title">Qualification Details</h4>
                        <p class="category">information about degree, year, university etc.</p>
                    </div>
                    <div class="card-content table-responsive">
                        <table class="table">
                            <thead class="text-primary">
                                <th>Degree</th>
                                <th>University</th>
                                <th>Pass Class</th>
                                <th>Year of Degree Awarded</th>
                                <th>Area of Specialization</th>
                            </thead>
                            <tbody>
                                <% loadInTableModular(tableNames[0])%>
                            </tbody>
                        </table>
                    </div>
                <a href = "/faculty/generateexcel/1/2">
                <button class="btn btn-primary pull-left" 
                <% if( GetParam ){%>
                disabled = true;
                <%}%>
                 style="margin:13px">Generate Excel file</button>
                </a>
                <button class="btn btn-primary pull-right" 
                <% if( GetParam ){%>
                disabled = true;
                <%}%>
                 data-toggle="modal" data-target="#myModalInsert" onclick = "fillDataModularInsert('<%=tableNames[0]%>', <%= JSON.stringify(columnSchema[0]) %>)" style="margin:13px">Insert</button>
                </div>
            </div>
        </div>
    </div>
</div>



<% include ../partials/footer %>
</script>
